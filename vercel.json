{
    "functions": {
        "api/*.php": {
            "runtime": "https://github.com/vercel-community/php.git"
        }
    },
    "routes": [{
        "src": "/api/info.php",
        "dest": "/api/info.php"
    },{
        "src": "/usr/(.*)",
        "dest": "/usr/$1"
    },{
        "src": "/admin/(.*).php",
        "dest": "/api/admin.php"
    },{
        "src": "/admin/(.*)",
        "dest": "/admin/$1"
    },{
        "src": "/(.*)",
        "dest": "/api/index.php"
    }]
}